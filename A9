package Arrays;

import java.util.Scanner;

public class A9 {
     public static void main(String[] args) {
        Scanner in=new Scanner(System.in);
        System.out.print("Enter the number of elements: ");
        int n=in.nextInt();
        int [] arr=new int[n];
        System.out.println("Enter the elements: ");
        for(int i=1;i<n;i++){
            arr[i]=in.nextInt();
        }
        int miss=0;
        for (int i = 1; i <n; i++) {
            if(arr[i]==i) continue;
            else miss=i;
        }
        System.out.println(miss);
    }
}
