package Recursion;

import java.util.Scanner;

public class R2 {
    public static void main(String[] args) {
        Scanner in=new Scanner(System.in);
        System.out.print("Enter the number:");
        int n=in.nextInt();
        System.out.print("The numbers are: ");
        r2(n);
    }
    static void r2(int n){
        if(n<1) return;
        System.out.print(n + " ");
        r2(n-1);
    }
}
