package Recursion;
import java.util.*;
public class R1 {
    public static void main(String[] args) {
        Scanner in=new Scanner(System.in);
        System.out.print("Enter the number:");
        int n=in.nextInt();
        r1(1,n);
    }
    static void r1(int i,int n){
        if(i>n) return;
        System.out.println("Raj");
        r1(i+1,n); 
    }

}
