package Hashing;
import java.util.*;
public class H1 {
    public static void main(String[] args) {
        Scanner in=new Scanner(System.in);
        System.out.print("Enter the no. of elements in the the array: ");
        int n=in.nextInt();
        int [] arr=new int[n];
        System.out.print("Enter the array element: ");
        for(int i=0;i<n;i++){
            arr[i]=in.nextInt();
        }
        System.out.println(Arrays.toString(arr));

        //precompute
        HashMap<Integer,Integer> hash=new HashMap<>();
        for(int i=0;i<n;i++){
            hash.put(arr[i],hash.getOrDefault(arr[i],0)+1);
        }
        //iterate
        System.out.println("Frequency of elements: ");
        for(Map.Entry<Integer,Integer> it:hash.entrySet()){
            System.out.println(it.getKey()+"->" +it.getValue());
        }
        
        //fetch
        System.out.print("Enter the number of the times you want to search the numbers: ");
        int t=in.nextInt();
        for(int i=0;i<t;i++){
            System.out.print("Enter the number you want to search: ");
            int n0=in.nextInt();
            System.out.printf("No. of times %d occurs: %d\n",n0,hash.getOrDefault(n0,0));
        }
    }
}
